[general]
abbreviate-tab-names=true
compat-delete='delete-sequence'
display-n=0
history-size=10000
infinite-history=false
max-tab-name-length=50
mouse-display=true
open-tab-cwd=true
prompt-on-quit=true
quick-open-command-line='gedit %(file_path)s'
restore-tabs-notify=true
restore-tabs-startup=true
save-tabs-when-changed=true
scroll-keystroke=true
set-window-title=true
start-at-login=true
use-default-font=false
use-popup=false
use-scrollbar=true
use-trayicon=true
window-halignment=0
window-height=71
window-losefocus=false
window-refocus=false
window-tabbar=true
window-valignment=0
window-width=100

[keybindings/local]
close-tab='<Primary><Shift>w'
move-tab-left='<Primary>KP_Page_Up'
move-tab-right='<Primary>KP_Next'
new-tab='<Primary>t'
new-tab-home='<Primary><Shift>h'
next-tab='<Primary>Right'
previous-tab='<Primary>Left'
rename-current-tab='<Primary><Shift>r'

[style/background]
transparency=64

[style/font]
allow-bold=true
bold-is-bright=true
palette='#000000000000:#cdcb00000000:#0000cdcb0000:#cdcbcdcb0000:#1e1a908fffff:#cdcb0000cdcb:#0000cdcbcdcb:#e5e2e5e2e5e2:#4ccc4ccc4ccc:#ffff00000000:#0000ffff0000:#ffffffff0000:#46458281b4ae:#ffff0000ffff:#0000ffffffff:#ffffffffffff:#ffffffffffff:#000000000000'
palette-name='Xterm'
style='JetBrainsMono Nerd Font 10'
